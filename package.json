  "dependencies": {
    "influx": "^5.0.0-alpha.4",
    "uuid": "^3.0.1",
    "vss-web-extension-sdk": "^5.131.0",
    "vsts-task-lib": "^2.0.6",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@types/node": "^9.6.4",
    "@types/q": "^1.5.0",
    "@types/xml2js": "^0.4.2"
  }
}
